<Window x:Class="MaciejKloda_140054.DockPanel"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MaciejKloda_140054"
        mc:Ignorable="d"
        Title="DockPanel" MinHeight="450" MinWidth="500" Height="450" Width="500">
    <DockPanel>
        <Button Content="Bottom" DockPanel.Dock="Bottom" Height="50"/>
        <Button Content="Left" DockPanel.Dock="Left" Width="50"/>
        <Button Content="Right" DockPanel.Dock="Right" Width="50"/>
        <Button Content="Top" DockPanel.Dock="Top"  Background="Red" Height="100"/>
        <Button Content="Top2" DockPanel.Dock="Top" Background="green" Width="150" Height="150"/>
        <Button Content="Right2" DockPanel.Dock="Right" Height="75" Width="100"/>
        <Button Content="Right3" DockPanel.Dock="Right" Height="75" Width="100"/>
        <Button Content="Top3" DockPanel.Dock="Top" Background="blue" Height="50"/>
        <Button Content="Fill" />
    </DockPanel>
</Window>
